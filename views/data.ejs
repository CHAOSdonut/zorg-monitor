<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<%- include header.ejs %>
<body>
<h1><%= title %></h1>
    <table class="table table-striped table-hover">
        <thead class="thead-dark">
        <tr><th scope="col">Data: </th></tr>
        </thead>
        <tbody>
        <% str.forEach(function(o) { %>
        <tr><td> <%= o.name %> </td></tr>
        <% }); %>
        </tbody>
    </table>
</body>
</html>
